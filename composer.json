{
    "name": "geesu/scout_apm_php",
    "description": "A PHP package connecting to the scout core agent API",
    "keywords": [
        "league",
        "package"
    ],
    "homepage": "https://github.com/geesu/scout_apm_php",
    "license": "MIT",
    "authors": [
        {
            "name": ":Geesu",
            "email": "beckerbi@gmail.com",
            "homepage": "https://github.com/geesu/scout_apm_php",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Scout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scout\\Core\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
